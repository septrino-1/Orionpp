       <string notr="true">/* TabBar 上的每个 tab */
QTabBar::tab {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #e0e0e0, stop:1 #c8c8c8); /* 浅灰渐变 */
    color: #333333;                     /* 文字深灰 */
    padding: 8px 18px;
    border-top-left-radius: 6px;
    border-top-right-radius: 6px;
    margin-right: 2px;
    border: 1px solid transparent;      /* 防止边框突兀 */
}

/* 鼠标悬停效果 */
QTabBar::tab:hover {
    background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                                stop:0 #d8d8d8, stop:1 #bfbfbf); /* 悬停渐变更柔和 */
}

/* 选中状态的 tab */
QTabBar::tab:selected {
    background: #f8f8f8;                /* 更柔和的选中背景 */
    border-bottom: 3px solid #4CAF50;   /* 绿色选中标记 */
    color: #000000;                      /* 文字黑色 */
    margin-top: 0px;                     /* 与未选中 tab 对齐 */
}

/* 未选中状态 tab 的上边距微调 */
QTabBar::tab:!selected {
    margin-top: 3px;
}

/* TabBar 默认靠左排列 */
QTabWidget::tab-bar {
    alignment: left;
}
</string>
