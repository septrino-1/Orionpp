    <string notr="true">/* ---------- 菜单栏 ---------- */
QMenuBar {
    background-color: #f5faff;      /* 浅白，微蓝调 */
    color: #1a1a1a;                 /* 深灰字体，保证可读性 */
    padding: 4px;
}

QMenuBar::item {
    background: transparent;        /* 默认透明背景 */
    padding: 6px 12px;              /* 内边距 */
}

QMenuBar::item:selected {
    background-color: #cce4ff;      /* 鼠标悬停：浅蓝高亮 */
    color: #003366;                 /* 深蓝文字 */
}

/* ---------- 下拉菜单 ---------- */
QMenu {
    background-color: #f8fbff;      /* 下拉菜单背景：浅白微蓝 */
    color: #1a1a1a;                 /* 文字颜色深灰 */
    border: 1px solid #aaccee;      /* 边框：淡蓝 */
}

QMenu::item {
    padding: 6px 20px;
}

QMenu::item:selected {
    background-color: #cce4ff;      /* 下拉菜单悬停：浅蓝高亮 */
    color: #003366;                 /* 深蓝文字 */
}
