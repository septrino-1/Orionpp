   <property name="styleSheet">
    <string notr="true">/* 工具栏背景 */
QToolBar {
    background-color: qlineargradient(
        x1:0, y1:0, x2:0, y2:1,
        stop:0 #dce3ed,   /* 上方淡蓝灰 */
        stop:1 #b0b8c8    /* 下方灰蓝色 */
    );
    spacing: 2px;          /* 工具项间距 */
    padding: 2px;          /* 内边距 */
    border: none;          /* 去掉边框 */
}

/* 工具栏按钮 */
QToolButton {
    background-color: transparent;  /* 默认透明 */
    color: #2e3a50;                /* 深灰蓝文字/图标 */
    border: 1px solid transparent;
    border-radius: 4px;
    padding: 4px;
}

/* 鼠标悬停 */
QToolButton:hover {
    background-color: rgba(50, 100, 180, 0.15); /* 柔和悬停蓝 */
}

/* 按下状态 */
QToolButton:pressed {
    background-color: rgba(50, 100, 180, 0.25); /* 略深悬停 */
}

/* 下拉按钮箭头 */
QToolButton::menu-indicator {
    image: none;  /* 隐藏默认小箭头 */
}
</string>
